    "eslint": "^7.12.1",
    "eslint-config-cheminfo": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
